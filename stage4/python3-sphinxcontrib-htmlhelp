#!/bin/bash 
set -e


TMP=${TMP:-$/tmp/work}
LOG=${LOG:-$HOME/logs/stage4}
SRC=${SRC:-$LFS/sources}

name=python3-sphinxcontrib-htmlhelp
version=1.0.3

mkdir -p $TMP $LOG

tar xf $SRC/sphinxcontrib-htmlhelp-$version.tar.gz -C $TMP

{ time \
   {

    cd $TMP/sphinxcontrib-htmlhelp-$version
    
	python3 setup.py install --optimize=1
    
    }
} 2>&1 | tee $name.log
    
[ $PIPESTATUS = 0 ] && mv $name.log $LOG || exit $PIPESTATUS

rm -fr $TMP
